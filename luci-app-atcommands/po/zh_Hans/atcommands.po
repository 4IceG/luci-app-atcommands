msgid "Modem"
msgstr "移动通信"

msgid "AT Commands"
msgstr "AT 指令"

msgid "Configuration"
msgstr "配置"

msgid "Please specify the AT command to send"
msgstr "请指定要发送的AT指令"

msgid "Please set the port for communication with the modem"
msgstr "请设置与模块通信的端口"

msgid "User AT commands"
msgstr "用户 AT 指令"

msgid "Command to send"
msgstr "要发送的指令"

msgid "Clear form"
msgstr "清除表单"

